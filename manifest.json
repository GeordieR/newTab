{
    
    "manifest_version": 2,
    "name": "Quick Earth",
    "description": "Displays quick links in front of Google Pretty Earth Images",
    "version": "1",
    "author": "Geordie Rogers",
    "icons": { "16": "ico/icon16.png",
           "48": "ico/icon48.png",
          "128": "ico/icon128.png" },
    "permissions" : ["activeTab"],
    "chrome_url_overrides" : {
        "newtab": "newtab.html"
    }
 }